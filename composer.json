{
  "name": "kraftner/kraftner-wp-torro-forms-submission-source",
  "description": "Plugin for Torro Forms that adds a field that stores the source of the submission.",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin",
    "extension",
    "torro forms",
    "forms",
    "form builder",
    "api"
  ],
  "homepage": "https://kraftner.com",
  "authors": [
    {
      "name": "Thomas KrÃ¤ftner",
      "email": "thomas@kraftner.com",
      "homepage": "https://kraftner.com"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0",
    "api-api/structure-wordpress": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": ">4.8.20 <6.0",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "^1",
    "wimg/php-compatibility": "^8",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
  },
  "archive": {
    "exclude": [
      "/tests",
      "/.*",
      "/docker*",
      "/Docker*",
      "/gulpfile.js",
      "/package*.json",
      "/php*.xml"
    ]
  },
  "scripts":     {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  }
}
